{
    "name": "dbml-language",
    "displayName": "DBML Language",
    "description": "Syntax highlighting for DBML Language",
    "version": "0.0.1",
    "publisher": "duynvu",
    "repository": "https://github.com/duynvu/DBML-Highlighter.git",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "dbml",
            "aliases": ["DBML", "dbml"],
            "extensions": [".dbml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "dbml",
            "scopeName": "source.dbml",
            "path": "./syntaxes/dbml.tmLanguage.json"
        }]
    }
}